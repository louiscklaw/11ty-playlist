<ol class="" >

  {% for post in postslist | reverse %}
    <li>
      <a href="{{ post.url }}">
      <div class="p-4 mt-4
        rounded-xl
        bg-slate-50
        border border-slate-150
        prose
        prose-p:mb-0
        prose-h2:mb-2
      ">
        {% if post.data.title %}
          <h2>{{ post.data.title }}</h2>
        {% else %}
          <code>{{ post.url }}</code>
        {% endif %}
        <p>Read Install and Update instructions here</p>
        <div>
          <time datetime="{{ post.date | htmlDateString }}">
            {{ post.date | readableDate("LLLL yyyy") }}
          </time>
          · 3 min · Aditya Telange
        </div>
      </div>
      </a>
    </li>
  {% endfor %}

</ol>
